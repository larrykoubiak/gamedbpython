  <Parsers>
    <WebPageParser>
      <Name>Search Results</Name>
      <URL>http://www.mobygames.com/search/quick?q=GAMENAME&amp;p=PLATFORMID&amp;search=Go&amp;sFilter=1&amp;sG=on</URL>
      <Parser>
        <StartNode>searchResults</StartNode>
        <Table>
          <Name>Search Results</Name>
          <Columns>
            <Column>MatchRating</Column>
            <Column>Platform</Column>
            <Column>Title</Column>
            <Column>URL</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>./div//div[@class='searchResult']</Path>
              <EndOfParsing>false</EndOfParsing>
              <Patterns>
                <Pattern>
                  <Path>./div[@class='searchNumber']</Path>
                  <Regex>&lt;div class=&quot;searchNumber&quot;&gt;(?&lt;MatchRating&gt;.+)\.&lt;\/div&gt;</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>./div[@class='searchData']</Path>
                  <EndOfParsing>false</EndOfParsing>
                  <Patterns>
                      <Pattern>
                        <Path>./div[@class='searchTitle']/a</Path>
                        <Regex>&lt;a href=&quot;(?&lt;URL&gt;.+?)&quot;&gt;(?&lt;Title&gt;.+?)&lt;\/a&gt;</Regex>
                        <EndOfParsing>false</EndOfParsing>
                      </Pattern>
                      <Pattern>
                        <Path>./div[@class='searchDetails']/span</Path>
                        <Regex>&lt;span .+?&gt;(?&lt;Platform&gt;.+?)\s\(&lt;em&gt;.*&lt;\/em&gt;\)&lt;\/span&gt;</Regex>
                        <EndOfParsing>true</EndOfParsing>
                      </Pattern>
                  </Patterns>
                </Pattern>
              </Patterns>
            </Pattern>
          </Patterns>
        </Table>
      </Parser>
    </WebPageParser>
    <WebPageParser>
      <Name>Game Data</Name>
      <URL>http://www.mobygames.comGAMEURL</URL>
      <Parser>
        <StartNode>main</StartNode>
        <Table>
          <Name>Genres</Name>
          <Columns>
            <Column>Genre</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//div[@id="coreGameGenre"]/div/div/a</Path>
              <Regex>&lt;a href=.+?&gt;(?&lt;Genre&gt;.+?)&lt;\/a&gt;</Regex>
              <EndOfParsing>true</EndOfParsing>
            </Pattern>
          </Patterns>
        </Table>
        <Table>
          <Name>Plot</Name>
          <Columns>
            <Column>Plot</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//div[@class="col-md-8 col-lg-8"]/div[@id="ctrq"]/following-sibling::h2[text()="Description"]/following-sibling::node()[not(self::div) and not(preceding-sibling::div[@class="sideBarLinks"])]/descendant-or-self::text()</Path>
              <Regex>(?ms)^(?:&lt;.+?&gt;)?(?&lt;Plot&gt;.+?)(?:&lt;\/.+?&gt;|$)</Regex>
              <EndOfParsing>true</EndOfParsing>
            </Pattern>
          </Patterns>
        </Table>
        <Table>
          <Name>Developers</Name>
          <Columns>
            <Column>Developer</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//div[@id="coreGameRelease"]/div[text()="Developed by"]/following-sibling::div[1]/a</Path>
              <Regex>&lt;a href=.+?&gt;(?&lt;Developer&gt;.+?)&lt;\/a&gt;</Regex>
              <EndOfParsing>true</EndOfParsing>
            </Pattern>
          </Patterns>
        </Table>
        <Table>
          <Name>Ratings</Name>
          <Columns>
            <Column>Rating</Column>
            <Column>Max</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//div[@id="coreGameScore"]/div[1]/text()</Path>
              <Regex>^(?&lt;Rating&gt;[\d|\.]+?)$</Regex>
              <EndOfParsing>true</EndOfParsing>
            </Pattern>
          </Patterns>
        </Table>
      </Parser>
    </WebPageParser>
    <WebPageParser>
      <Name>Release Data</Name>
      <URL>http://www.mobygames.comGAMEURL/release-info</URL>
      <Parser>
        <StartNode>main</StartNode>
        <Table>
          <Name>Releases</Name>
          <Columns>
            <Column>Publisher</Column>
            <Column>Developer</Column>
            <Column>Distributor</Column>
            <Column>Name</Column>
            <Column>Region</Column>
            <Column>ProductId</Column>
            <Column>Barcode</Column>
            <Column>ReleaseDate</Column>
            <Column>Rating</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//div[@class="floatholder relInfo"][div[@class="relInfoTitle"][text()="Country" or text()="Countries"]]/div[@class="relInfoDetails"]/span/img</Path>
              <EndOfParsing>false</EndOfParsing>
              <Patterns>
                <Pattern>
                  <Path>parent::span/parent::div/parent::div/following-sibling::div[div[@class="relInfoTitle"][text()="Nintendo Media PN"]]/div[@class="relInfoDetails"]/text()</Path>
                  <Regex>(?&lt;ProductId&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>parent::span/parent::div/parent::div/following-sibling::div[div[@class="relInfoTitle"][text()="EAN-13" or text()="UPC/A"]]/div[@class="relInfoDetails"]/text()</Path>
                  <Regex>(?&lt;Barcode&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>parent::span/parent::div/parent::div/following-sibling::div[div[@class="relInfoTitle"][text()="Release Date"]]/div[@class="relInfoDetails"]/text()</Path>
                  <Regex>(?&lt;ReleaseDate&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>parent::span/parent::div/parent::div/parent::div/preceding-sibling::div[div[text()="Distributed by"]][1]/a/text()</Path>
                  <Regex>(?&lt;Distributor&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>parent::span/parent::div/parent::div/parent::div/preceding-sibling::div[div[text()="Developed by"]][1]/a/text()</Path>
                  <Regex>(?&lt;Developer&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>                
                <Pattern>
                  <Path>parent::span/parent::div/parent::div/parent::div/preceding-sibling::div[div[text()="Published by"]][1]/a/text()</Path>
                  <Regex>(?&lt;Publisher&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>parent::span/text()</Path>
                  <Regex>^\s*(?&lt;Region&gt;.+?),?$</Regex>
                  <EndOfParsing>true</EndOfParsing>
                </Pattern>
              </Patterns>
            </Pattern>
          </Patterns>
        </Table>
      </Parser>
    </WebPageParser>
    <WebPageParser>
      <Name>Release Images</Name>
      <URL>http://www.mobygames.comGAMEURL/cover-art</URL>
      <Parser>
        <StartNode>main</StartNode>
        <Table>
          <Name>Images</Name>
          <Columns>
            <Column>Type</Column>
            <Column>URL</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//div[@class="row"][preceding-sibling::div[@class="coverHeading"]//td[text()="Country" or text()="Countries"]/following-sibling::td[2]/span[text() = "RELEASEREGION "]][1]//a[@class="thumbnail-cover"]</Path>
              <Regex>style=&quot;background-image:url\((?&lt;URL&gt;.+?)\);&quot;</Regex>
              <EndOfParsing>false</EndOfParsing>
              <Patterns>
                <Pattern>
                  <Path>parent::div/following-sibling::div[@class="thumbnail-cover-caption"][1]/p/text()</Path>
                  <Regex>(?:^|(?:[.!?]\s))(?&lt;Type&gt;\w+)</Regex>
                  <EndOfParsing>true</EndOfParsing>
                </Pattern>                
              </Patterns>
            </Pattern>
          </Patterns>
        </Table>
      </Parser>
    </WebPageParser>
    <WebPageParser>
      <Name>Game List Pages</Name>
      <URL>http://www.mobygames.com/browse/games/PLATFORMURL/list-games/</URL>
      <Parser>
        <StartNode>main</StartNode>
        <Table>
          <Name>Game List Pages</Name>
          <Columns>
            <Column>Current</Column>
            <Column>Pages</Column>
            <Column>Start</Column>
            <Column>End</Column>
            <Column>Total</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//td[@class="mobHeaderItems"]/text()</Path>
              <Regex>\(items (?&lt;Start&gt;\d+?)-(?&lt;End&gt;\d+?) of (?&lt;Total&gt;\d+?)\)</Regex>
              <EndOfParsing>false</EndOfParsing>
            </Pattern>
            <Pattern>
              <Path>//td[@class="mobHeaderPage"]/text()</Path>
              <Regex>Viewing (?:Page (?&lt;Current&gt;\d+?) of (?&lt;Pages&gt;\d+?)|all (?&lt;Total&gt;\d+?) items)$</Regex>
              <EndOfParsing>true</EndOfParsing>
            </Pattern>
          </Patterns>          
        </Table>
      </Parser>
    </WebPageParser>
    <WebPageParser>
      <Name>Game List</Name>
      <URL>http://www.mobygames.com/browse/games/PLATFORMURL/offset,OFFSET/so,0a/list-games/</URL>
      <Parser>
        <StartNode>mof_object_list</StartNode>
        <Table>
          <Name>Game List</Name>
          <Columns>
            <Column>Title</Column>
            <Column>Year</Column>
            <Column>Publisher</Column>
            <Column>Genre</Column>
            <Column>URL</Column>
          </Columns>
          <Patterns>
            <Pattern>
              <Path>//tr</Path>
              <EndOfParsing>false</EndOfParsing>
              <Patterns>
                <Pattern>
                  <Path>td[1]/a[1]</Path>
                  <Regex>&lt;a href=&quot;(?&lt;URL&gt;.+?)&quot;&gt;(?&lt;Title&gt;.+?)&lt;\/a&gt;</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>td[2]/a[1]/text()</Path>
                  <Regex>(?&lt;Year&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>td[3]/a[1]/text()</Path>
                  <Regex>(?&lt;Publisher&gt;.*)</Regex>
                  <EndOfParsing>false</EndOfParsing>
                </Pattern>
                <Pattern>
                  <Path>td[4]/a[1]/text()</Path>
                  <Regex>(?&lt;Genre&gt;.*)</Regex>
                  <EndOfParsing>true</EndOfParsing>
                </Pattern>          
              </Patterns>
            </Pattern>
          </Patterns>
        </Table>
      </Parser>
    </WebPageParser>
  </Parsers>
</WebScraper>
