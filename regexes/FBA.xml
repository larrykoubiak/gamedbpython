<?xml version="1.0"?>
<GameDBRegexes>
	<ReleaseGroup name="No-Intro">
    	<Regex>
			<Name>System</Name>
			<Pattern>^(?P<Manufacturer>.+?)\s-\s(?P<Name>.+?)(?:\s\((?P<Format>.+?)\))?(?:\s(?P<DatType>Parent-Clone)|$)</Pattern>
		</Regex>
		<Regex>
			<Name>DatDate</Name>
			<Pattern>(?P<Year>\d{4})(?P<Month>\d{2})(?P<Day>\d{2})-(?P<Hour>\d{2})(?P<Minute>\d{2})(?P<Second>\d{2})</Pattern>
		</Regex>
    	<Regex>
			<Name>Software</Name>
			<Pattern>^(?:\[(?P<BIOS>BIOS)\]\s)?(?P<Name>[^\(]+)\s(?:.+?)(?:\((?P<Type>Addon|Coverdisk|Diskmag|Program).*\)|\)$|\]$)</Pattern>
		</Regex>
    	<Regex>
			<Name>DatTuple</Name>
			<Pattern>\t*(.+?) \"?(.+?)\"?$</Pattern>
		</Regex>
    	<Regex>
			<Name>Rom</Name>
			<Pattern>\( (?:name \"(?P<name>.*)\" )?(?:size (?P<size>.+?) )?(?:crc (?P<crc>.+?) )?(?:md5 (?P<md5>.+?) )?(?:sha1 (?P<sha1>.+?) )?(?:status (?P<status>.+?) )?\)</Pattern>
		</Regex>
    	<Regex>
			<Name>DevStatus</Name>
			<Pattern>\(.+?(?P<DevStatus>Beta|Proto|Sample)(?:\s\d)?\)</Pattern>
		</Regex>
    	<Regex>
			<Name>Demo</Name>
			<Pattern>(?:\((?=[^\(]*(?:, [^\(]*)*\))|(?<=, )\G|[\(]*)(?P<Demo>Demo|Promo|Budget)(?:, |[^\(]*\))</Pattern>
		</Regex>
    	<Regex>
			<Name>Region</Name>
			<Pattern>(?:\((?=[^,|\)]+(?:, [^,|\)]+)*\))|(?<=, )\G)(?P<Region>Australia|Brazil|Canada|China|France|Germany|Greece|Hong Kong|Italy|Japan|Korea|Netherlands|Norway|Russia|Spain|Sweden|USA|United Kingdom|Unknown|Asia|Europe|World)(?:, |\))</Pattern>
		</Regex>
    	<Regex>
			<Name>Compilation</Name>
			<Pattern>\((?:[^\)]*?)?\s?(?P<Compilation>Compilation)(?: - )?(?:[^\)]*?)?\)</Pattern>
		</Regex>
    	<Regex>
			<Name>Language</Name>
			<Pattern>(?:\((?=\w+(?:,\w+)*\))|(?<=,)\G)(?P<Language>En|Ja|Fr|De|Es|It|Nl|Pt|Sv|No|Da|Fi|Zh|Ko|Pl|Ru|,)(?:,|\))</Pattern>
		</Regex>
    	<Regex>
			<Name>Version</Name>
			<Pattern>\(.*?v(?P<Version>[\d|\.]+\w?)\s?(?:[^\)]*)\)</Pattern>
		</Regex>
    	<Regex>
			<Name>Revision</Name>
			<Pattern>\(.*?Rev (?P<Revision>[\d|\w|\.]+)(?:[^\)]*)?\)</Pattern>
		</Regex>
    	<Regex>
			<Name>License</Name>
			<Pattern>\((?P<License>Unl)\)</Pattern>
		</Regex>
    	<Regex>
			<Name>DumpStatus</Name>
			<Pattern>\[(?<BadDump>b)\]</Pattern>
		</Regex>	
	</ReleaseGroup>
</GameDBRegexes>